  author = {Murphy, Rachel M. and Klopotowska, Joanna E. and de Keizer, Nicolette F. and Jager, Kitty J. and Leopold, Jan Hendrik and Dongelmans, Dave A. and {Abu-Hanna}, Ameen and Schut, Martijn C.},
  year = {2023},
